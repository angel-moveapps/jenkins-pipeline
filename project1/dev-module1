pipeline {
  agent any
  parameters {
      gitParameter name: 'BRANCH_TAG', 
                     type: 'PT_BRANCH_TAG',
                     defaultValue: 'master',
 					 useRepository: '.*dummy-proyect.git'
  }
  stages {
    stage('Checkout3') {
      steps {
        echo 'checkout'
        git(url: 'https://github.com/angel-moveapps/dummy-proyect.git', branch: 'master', credentialsId: 'github')
      }
    }

    stage('Listing workspace') {
      steps {
        sh 'ls'
      }
    }

    stage('Build') {
      steps {
        echo 'this is the build'
      }
    }

  }
}